#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/workspace/wai-sdk/node_modules/.pnpm/@anthropic-ai+sdk@0.68.0_zod@3.25.76/node_modules/@anthropic-ai/sdk/bin/node_modules:/home/runner/workspace/wai-sdk/node_modules/.pnpm/@anthropic-ai+sdk@0.68.0_zod@3.25.76/node_modules/@anthropic-ai/sdk/node_modules:/home/runner/workspace/wai-sdk/node_modules/.pnpm/@anthropic-ai+sdk@0.68.0_zod@3.25.76/node_modules/@anthropic-ai/node_modules:/home/runner/workspace/wai-sdk/node_modules/.pnpm/@anthropic-ai+sdk@0.68.0_zod@3.25.76/node_modules:/home/runner/workspace/wai-sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/workspace/wai-sdk/node_modules/.pnpm/@anthropic-ai+sdk@0.68.0_zod@3.25.76/node_modules/@anthropic-ai/sdk/bin/node_modules:/home/runner/workspace/wai-sdk/node_modules/.pnpm/@anthropic-ai+sdk@0.68.0_zod@3.25.76/node_modules/@anthropic-ai/sdk/node_modules:/home/runner/workspace/wai-sdk/node_modules/.pnpm/@anthropic-ai+sdk@0.68.0_zod@3.25.76/node_modules/@anthropic-ai/node_modules:/home/runner/workspace/wai-sdk/node_modules/.pnpm/@anthropic-ai+sdk@0.68.0_zod@3.25.76/node_modules:/home/runner/workspace/wai-sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@anthropic-ai/sdk/bin/cli" "$@"
else
  exec node  "$basedir/../@anthropic-ai/sdk/bin/cli" "$@"
fi
