#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/workspace/node_modules/.pnpm/vm2@3.10.0/node_modules/vm2/bin/node_modules:/home/runner/workspace/node_modules/.pnpm/vm2@3.10.0/node_modules/vm2/node_modules:/home/runner/workspace/node_modules/.pnpm/vm2@3.10.0/node_modules:/home/runner/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/workspace/node_modules/.pnpm/vm2@3.10.0/node_modules/vm2/bin/node_modules:/home/runner/workspace/node_modules/.pnpm/vm2@3.10.0/node_modules/vm2/node_modules:/home/runner/workspace/node_modules/.pnpm/vm2@3.10.0/node_modules:/home/runner/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vm2/bin/vm2" "$@"
else
  exec node  "$basedir/../vm2/bin/vm2" "$@"
fi
